{
	"author": {
		"name": "Baptiste Pillot",
		"email": "baptiste@pillot.fr"
	},
	"dependencies": {
		"@itrocks/schema": "latest",
		"mariadb": "^3.4"
	},
	"description": "Extracts a MySQL table structure and converts it into a table schema",
	"devDependencies": {
		"typescript": "~5.9"
	},
	"engines": {
		"node": ">=18"
	},
	"exports": {
		"import": "./esm/mysql-to-schema.js",
		"require": "./cjs/mysql-to-schema.js"
	},
	"files": [
		"LICENSE",
		"README.md",
		"*/*.d.ts",
		"*/*.js"
	],
	"homepage": "https://it.rocks",
	"keywords": [
		"backend",
		"it.rocks",
		"mapping",
		"mariadb",
		"mysql",
		"schema",
		"structure",
		"table"
	],
	"license": "LGPL-3.0-or-later",
	"name": "@itrocks/mysql-to-schema",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/itrocks-ts/mysql-to-schema.git"
	},
	"scripts": {
		"build": "npm run build:cjs && npm run build:esm",
		"build:cjs": "tsc -p tsconfig.cjs.json",
		"build:esm": "tsc -p tsconfig.esm.json"
	},
	"types": "./esm/mysql-to-schema.d.ts",
	"version": "0.0.10"
}
